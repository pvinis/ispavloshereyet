{
  "name": "ispavloshereyet",
  "version": "0.4.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start",
    "doctor": "bun x expo-doctor@latest",
    "doctor:fix": "expo install --check",
    "typecheck": "tsc --noEmit",
    "build:dev": "bun eas build --platform all --profile development --non-interactive",
    "build:devdevice": "bun eas build --platform all --profile development-device --non-interactive",
    "build:prod": "bun eas build --platform all --non-interactive --auto-submit",
    "devclient": "bun devclient:ios ; bun devclient:android",
    "devclient:ios": "bun eas build:run -p ios --latest",
    "devclient:android": "bun eas build:run -p android --latest",
    "submit": "bun eas submit --platform all",
    "eas": "bun x eas-cli@latest"
  },
  "dependencies": {
    "@instantdb/react-native": "^0.14.27",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/netinfo": "^11.4.1",
    "expo": "~52.0.2",
    "expo-constants": "~17.0.2",
    "expo-linking": "~7.0.2",
    "expo-location": "~18.0.1",
    "expo-router": "~4.0.2",
    "expo-status-bar": "~2.0.0",
    "expo-task-manager": "~12.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-native": "0.76.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.0.0",
    "react-native-ui-datepicker": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/lodash": "^4.17.13",
    "@types/luxon": "^3.4.2",
    "@types/react": "~18.3.12",
    "babel-loader": "^9.1.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.3"
  }
}
